
<tr class="data top_searches_list">
  <td colspan="4">
    <table class="searches">
      <thead>
        <td>Search Query</td>
        <td>Hits</td>
        <td>Embed Date</td>
        <td></td>
      </thead>
      <tbody>
        <% _.each(_.sortBy(TopSearches.models, function(query){ return -query.get('hits')}), function(query) { %>
          <tr>
            <td>
              <b><a href="<%= query.get('url') %>"><%= dc.inflector.truncate(query.get('search_query') || '', 100) %></a></b>
            </td>
            <td>
              <b><%= query.get('hits') %></b>
            </td>
            <td><%= query.get('first_recorded_date') %></td>
            <td class="last"></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  </td>
</tr>