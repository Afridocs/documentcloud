<% if (model.has('category')) { %>
  <div class="search_facet_category"><%= model.get('category') %>:</div>
<% } %>

<div class="search_facet_input_container">
  <input type="text" class="search_facet_input interface" value="<%= model.get('value') %>" />
</div>

<div class="search_facet_remove icon cancel_search"></div>