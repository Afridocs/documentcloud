<div class="select"></div>
<div class="hoverable">
  <img class="icon doc generic is_draggable noselect" src="<%= thumbnail_url %>" data-id="<%= id %>" />
  <div class="title">
    <a href="<%= document_viewer_url %>" class="doc_title noselect"><%= title %></a>&nbsp;<span class="<%= icon["class"] %>" title="<%= icon.title %>">&#65279;</span>&#8239;<span class="icon document_tool edit_glyph" title="Edit Document">&#65279;</span>
  </div>
</div>
<% if (annotation_count) { %>
  <span class="show_notes">
    <span class="icon cancel_search" title="hide notes"></span><span class="text_link"><span class="note_count"><%= annotation_count %></span> <%= Inflector.pluralize('note', annotation_count) %></span>
  </span>
  <div class="icon mini_note"></div>
<% } %>
<div class="description"><span class="noselect description_text"></span></div>
<div class="metalines interface">
  <span class="noselect">
    <% if (page_count) { %>
      <span class="page_count"><%= page_count + ' ' + Inflector.pluralize('page', page_count) %></span> -
    <% } %>
    <% if (source) { %>Source: <span class="quiet_link search_source"><%= source %></span> -<% } %>
    Contributed by:
    <span class="quiet_link search_account"><%= account_name %></span>,
    <span class="quiet_link search_group"><%= organization_name %></span> -
    <%= created_at %>
  </span>
</div>
<div class="notes"></div>
<div class="pages"></div>
