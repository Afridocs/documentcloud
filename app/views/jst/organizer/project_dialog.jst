<div class="text_input small">
  <div class="background">
    <div class="inner">
      <input name="title" id="project_title" type="text" value="<%= title %>" />
    </div>
  </div>
</div>
<label for="project_title">
  Title:
</label>

<div class="counts">
  <%= document_ids.length %> <%= Inflector.pluralize('document', document_ids.length) %>,
  <%= annotation_count %> <%= Inflector.pluralize('note', annotation_count) %>
</div>

<button class="delete warn">delete</button>
