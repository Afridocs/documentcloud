document_id               # Top-level directory
  document_id.json        # Document's Canonical JSON representation
  document_id.txt         # Full Text
  document_id.html        # The HTML representation of the document.
  document_id.pdf         # The PDF of the Document
  thumbnails              # Thumbnail images of the document's cover.
    size.jpg ...          # Each thumbnail has its size as part of the filename.
  page_text               # The UTF8 text of each page.
    page.txt              # Tagged by page number. (Starts at 1)
  page_images             # The image of each page.
    page_size.gif         # Filename includes both page number and size.
    